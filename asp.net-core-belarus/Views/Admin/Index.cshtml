@model List<string>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutIdentity.cshtml";
}

<h2>Site users list :</h2>
<br />
<ul>
    @if (Model.Count > 0)
    {

        @foreach (var user in Model)
        {
            <li>@user</li>
        }
    }
    else
    {
        <p> NO USERS</p>
    }
</ul>
